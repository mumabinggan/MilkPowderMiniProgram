/* pages/orderlist/index.wxss */
.container {
  width: 100%;
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0rpx;
}

.tabsview {
  width: 100%;
  position: fixed;
  z-index: 1000;
}

.scroll-view {
  padding-top: 90rpx;
  background-color: #F2F2F2;
  z-index: 100;
}

::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}